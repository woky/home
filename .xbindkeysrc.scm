(define b xbindkey)
(b '(mod4 o)                "fmlove love")
(b '(mod4 d)                "dmenu_run")
(b '(mod4 g)                "copyq toggle")
(b '(mod4 slash)            "clipscreen-open")
(b '(mod4 semicolon)        "clipscreen-save")
(b '(mod4 apostrophe)       "clipscreen-upload")
(b '(mod4 F8)               "systemctl suspend")
(b '(shift mod4 d)          "pavucontrol")
(b '(mod4 F10)              "cmus-remote -u")
(b '(mod4 F11)              "cmus-remote -r")
(b '(mod4 F12)              "cmus-remote -n")
(b '(shift mod4 F11)        "cmus-remote -k -30")
(b '(shift mod4 F12)        "cmus-remote -k +30")
(b '(control XF86AudioLowerVolume)  "cmus-remote -v -5%")
(b '(control XF86AudioRaiseVolume)  "cmus-remote -v +5%")
(b '(mod4 c)                "xclip -o|(echo -n '\"'; cat; echo -n '\"')|xclip")
(b '(XF86AudioMute)         "pactl set-sink-mute 2 toggle")
(b '(XF86AudioLowerVolume)  "pactl set-sink-volume 2 '-5%'")
(b '(XF86AudioRaiseVolume)  "pactl set-sink-volume 2 '+5%'")
(b '(mod4 KP_Add)           "websearch $(xclip -o)")
(b '(mod4 z)                "websearch $(xclip -o)")
